  "homepage": "https://github.com/notifius/php-wctp",
  "license": "MIT",
  "authors": [
    {
      "name": "Patrick Labbett",
      "email": "patrick@labbett.net",
      "homepage": "https://labbett.net",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.3|^8.0",
    "ext-SimpleXML": "^7.3|^8.0",
    "nesbot/carbon": "^2.57.0"
